{
  "name": "messengerapplication",
  "version": "1.0.0",
  "description": "Use websocket",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env,stage-0\"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "vigneshwaran0310@gmail.com",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "immutable": "^4.0.0-rc.12",
    "loadash": "^1.0.0",
    "moment": "^2.24.0",
    "mongodb": "^3.5.4",
    "morgan": "^1.9.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v17.1.0",
    "uws": "^100.0.1",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.6.0",
    "nodemon": "^1.12.1"
  }
}
